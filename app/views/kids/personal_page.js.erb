<%- if @params[:f] %>
  $(".cards ul").html("<%= escape_javascript(render 'kids/kids', :@kids => @kids, :@hidden => true) %>");
  $(".cards__filter").html("<%= escape_javascript(render 'kids/cards_filter', :@kids => @kids, params: @params) %>");
<%- else %>
  $(".cards ul").append("<%= escape_javascript(render 'kids/kids', :@kids => @kids, :@hidden => true) %>");
  $(".cards__filter").html("<%= escape_javascript(render 'kids/cards_filter', :@kids => @kids, params: @params) %>");
<%- end %>

<%- if @kids.last_page? %>
$('.cards__more').remove();
<% end %>